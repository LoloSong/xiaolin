/**index.wxss**/
page {
  padding-bottom: 114rpx;
  background-color: #74c3c1;
}
.container {
  position: relative;
}
.container .search_box {
  width: 100%;
  padding-top: 18rpx;
  padding-bottom: 34rpx;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 20;
  background-color: #fff;
  border-radius: 0rpx 0rpx 48rpx 48rpx;
  color: #b2b2b2;
  font-size: 26rpx;
}
.container .search_box .search {
  display: flex;
  align-items: flex-end;
  margin-top: 82rpx;
}
.container .search_box .search .ipu_left {
  margin-left: 64rpx;
  margin-right: 10rpx;
  flex: 1;
}
.container .search_box .search .ipu_left input {
  color: #b2b2b2;
  font-size: 26rpx;
}
.container .search_box .search .btn_search .iconfont {
  font-size: 48rpx;
  color: #464646;
  margin-right: 60rpx;
}
.container .search_box .search_result .result_item {
  margin-top: 28rpx;
  padding-left: 64rpx;
}
.container .gray_logo {
  width: 100%;
  height: 454rpx;
  background-color: #e6e6e6;
  position: fixed;
  top: 0;
  z-index: 10;
  border-radius: 0rpx 0rpx 48rpx 48rpx;
}
.container .gray_logo .logo {
  width: 310rpx;
  height: 148rpx;
  margin: 226rpx auto;
}
.container .con_box {
  margin-top: 548rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.container .con_box .btn_list {
  width: 278rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  color: #858585;
  background-color: #fff;
  border-radius: 20rpx;
}
.container .con_box .ranking_box {
  width: 444rpx;
  height: 410rpx;
  position: relative;
  margin-top: 72rpx;
}
.container .con_box .ranking_box .ranking_top {
  display: flex;
}
.container .con_box .ranking_box .ranking_top .rank_item {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: relative;
}
.container .con_box .ranking_box .ranking_top .rank_item .avatar {
  border-radius: 50%;
  overflow: hidden;
  margin-top: 8rpx;
  box-shadow: rgba(0, 0, 0, 0.05) 0px 0px 10rpx;
}
.container .con_box .ranking_box .ranking_top .rank_item .avatar image {
  border-radius: 50%;
  background-color: #f5f5f5;
}
.container .con_box .ranking_box .ranking_top .rank_item .icon {
  width: 76rpx;
  height: 96rpx;
}
.container .con_box .ranking_box .ranking_top .rank_two .avatar,
.container .con_box .ranking_box .ranking_top .rank_three .avatar {
  width: 46rpx;
  height: 46rpx;
}
.container .con_box .ranking_box .ranking_top .rank_frist .avatar {
  width: 51rpx;
  height: 51rpx;
}
.container .con_box .ranking_box .ranking_top .rank_two {
  width: 140rpx;
  top: 80rpx;
}
.container .con_box .ranking_box .ranking_top .rank_frist {
  flex: 1;
  top: 20rpx;
}
.container .con_box .ranking_box .ranking_top .rank_three {
  width: 130rpx;
  top: 130rpx;
}
.container .con_box .ranking_box .ranking_img {
  width: 100%;
  height: 220rpx;
  position: absolute;
  bottom: 0;
}
.container .con_box .list_box {
  margin-top: 38rpx;
  margin-bottom: 16rpx;
}
.container .con_box .list_box .list_item {
  background-color: #fff;
  width: 444rpx;
  height: 66rpx;
  line-height: 66rpx;
  border-radius: 12rpx;
  box-shadow: 0 0 10rpx 5rpx rgba(0, 0, 0, 0.05);
  margin-bottom: 38rpx;
  padding: 0rpx 30rpx;
  color: #808080;
}
